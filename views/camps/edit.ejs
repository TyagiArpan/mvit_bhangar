<%- include("../partials/header")%>
<div class="container x">
	<div class="row">
		<div class="xyz">
			<div class="row">
				<div class="col-lg-6 col-md-6 col-sm-6">
					<div style="width:20%;margin:25px;">
						<img  style="width:350px;height:500px;"src="https://wallpaperaccess.com/full/909804.jpg">
					</div>
				</div>
				<div class="col-lg-6 col-md-6 col-sm-6">
		<h1 style="text-align:center;color:#fff;margin-top:175px;">Edit</h1>
		<div style="width:50%;margin:0 auto;">
			<form  action="/camp/<%=camp._id%>" method="POST" enctype="multipart/form-data">
				<div class="form-group">
					<input class="form-control" type="text" name="name" value="<%=camp.name%>">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" name="phnum" value="<%=camp.phnum%>">
				</div>
				<div class="form-group">
					<input class="form-control" type="number" name="price" value="<%=camp.price%>" min="1" step="0.5">
				</div>
				<div class="form-group">
					<input class="form-control" type="file" name="camp[image]" value="<%=camp.image%>">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" name="descp" value="<%=camp.descp%>">
				</div>
				<button class="btn btn-large btn-block">Submit</button>
			</form>
		</div>	
	</div>		
</div>
			



